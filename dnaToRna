// This program turns DNA chain to RNA chain.

const dnaToRna = (dna) => {
  let rna = '';
  for (let index = 0; index < dna.length; index += 1) {
    if (dna[index] === 'G') rna += 'C';
    else if (dna[index] === 'C') rna += 'G';
    else if (dna[index] === 'T') rna += 'A';
    else if (dna[index] === 'A') rna += 'U';
    else return null;
  } return rna;
};
